{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/Movies",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/movies",
      "ServiceName": "movieservice",
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/Movies/{id}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/movies/{id}",
      "ServiceName": "movieservice",
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "UpstreamHttpMethod": [
        "Get",
        "Put",
        "Delete"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/Musics",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/musics",
      "ServiceName": "musicservice",
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/Musics/{id}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/musics/{id}",
      "ServiceName": "musicservice",
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "UpstreamHttpMethod": [
        "Get",
        "Put",
        "Delete"
      ]
    }
  ],
  "GlobalConfiguration": {
    "UseServiceDiscovery": true,
    "ReRouteIsCaseSensitive": true,
    "ServiceDiscoveryProvider": {
      "Type": "Eureka",
      "Host": "eureka",
      "Port": "8761"
    }
  }
}