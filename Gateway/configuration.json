{
    "ReRoutes": [
      {
        "DownstreamPathTemplate": "/api/Movies/{everything}",
        "DownstreamScheme": "http",
        "UpstreamPathTemplate": "/movie/{everything}",
        "ServiceName": "movieservice",
        "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete"]
      },
      {
        "DownstreamPathTemplate": "/api/Musics/{everything}",
        "DownstreamScheme": "http",
        "UpstreamPathTemplate": "/music/{everything}",
        "ServiceName": "musicservice",
        "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete"]
      }
    ],
    "GlobalConfiguration": {
        "UseServiceDiscovery": true,
        "ServiceDiscoveryProvider": {
            "Port": "8761",
            "Type": "Eureka",
            "Host":  "eureka"
        }
    }    
}
